<Alloy>
	<View id="profileView" backgroundColor="#B2DAF6" height="Ti.UI.SIZE" >
		<ScrollView layout="vertical" id = "scrollContainer">
			<ScrollableView id="scrollableView" showPagingControl="true" top = "0dp" height="200dp">
				<ImageView id = "cameraIcon" image="images/camera.png"/>
			</ScrollableView>
			<TextField id="emailAddress" class="textFieldClass"/>
			<View class = "sapClass"/>
			<TextField id="qualification" class="textFieldClass"/>
			<View class = "sapClass"/>
			<TextField id="experience" class="textFieldClass"/>
			<View class = "sapClass" id = "experienceSap"/>
			<TextField id="address_permanent" class="textFieldClass"/>
			<View class = "sapClass"/>
			<TextField id="city" class="textFieldClass"/>
			<View class = "sapClass"/>
			<TextField id="contactNo" class="textFieldClass"/>
			<View class = "sapClass"/>
			<TextField id="label" class="textFieldClass" editable="false" onClick="LinkedInClick" />
			<View class = "sapClass" id = "labelSap"/>
			<TextField id="courses" class="textFieldClass"/>
			<View class = "sapClass" id = "coursesSap"/>
			<View id="coursesDropDown"  class="dropDownClass"/>
			<TextField id="subjects" class="textFieldClass"/>
			<View class = "sapClass" id = "subjectsSap"/>
			<View id="subjectsDropDown"  class="dropDownClass"/>
			<View id="batchTimeDropDown" class="dropDownClass"/>
			<TextField id="aboutYourSelf" class="textFieldClass"/>
			<View class = "sapClass"/>
			<Button id="submit" onClick="onSubmit"/>
			<!--  -->
		</ScrollView>
	</View>
</Alloy>
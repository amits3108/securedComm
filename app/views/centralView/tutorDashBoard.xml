<Alloy>
	<View class="container">
		<View id="headingView">
			<View id="profileHeading" onClick="profileViewClick">
				<Label id="profile"/>
				<View id="profileHighlighter"/>
			</View>
			<View id="timelineHeading" onClick="timeLineViewClick">
				<Label id="timeLine"/>
				<View id="timeLineHighlighter"/>
			</View>
		</View>
		<ScrollableView id="scrollableView">
			<View id="profileView"/>
			<View id="timelineView"/>
		</ScrollableView>
	</View>
</Alloy>